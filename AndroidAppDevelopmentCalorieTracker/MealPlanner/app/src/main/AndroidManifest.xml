<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mealplanner">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MealPlanner">
        <activity android:name=".ViewEatOn"></activity>
        <activity android:name=".ViewEat" />
        <activity android:name=".Insert" />
        <activity android:name=".ViewMeal" />
        <activity android:name=".AddMeal" />
        <activity android:name=".ViewFood" />
        <activity android:name=".AddFood" />
        <activity android:name=".Planner" />
        <activity
            android:name=".CalorieActivity"
            android:label="Calorie Tracker" />
        <activity
            android:name=".MainActivity"
            android:label="Personalised">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>